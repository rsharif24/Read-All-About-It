<div id="container">
      <header>
            <h1>Read All About It!</h1>
      </header>
      <nav class="navbar navbar-expand-lg navbar-light bg-light">
            <ul class="navbar-nav">
                   <li class="nav-item">
                        <a class="nav-link" href="#">Home</a>
                  </li>
                  <li class="nav-item">
                        <a class="nav-link" href="/saved">Saved Articles</a>
                  </li>
                  <li class="nav-item">
                        <a class="nav-link" href="#">Scrape New Articles</a>
                  </li>
            </ul>
      </nav>
      <div id="wrapper">
            <ul>
                  {{#each Article}}
                  <li>
                        <h3>{{this.title}}</h3>
                        <p id="articleLink" article-id="{{this._id}}">{{this.link}}</p>
                        <button id="saveButton" class="btn btn-default">Save Article</button>
                        
                  </li>
                  <br>
                  {{/each}}
            </ul>
      </div>

</div>